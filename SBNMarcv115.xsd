		<xsd:annotation>
			<xsd:documentation>Codice routine applicata per il controllo di similitudine
			000=base 001=massima 003=minima
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:length value="3"/>
		</xsd:restriction>
